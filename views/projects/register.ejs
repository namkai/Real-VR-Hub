<% include ../partials/header %>

<div class="container center">

  <h2>Sign Up!</h2>
  <br>


<div class="row" id="namkai">
  <div class="col s2">

  </div>
   <form class="col s8" action="/register" method="POST">
     <div class="row">
       <div class="input-field col s6">
         <input id="first_name" type="text" class="validate" name="username">
         <label for="first_name">First Name</label>
       </div>
       <div class="input-field col s6">
         <input id="last_name" type="text" class="validate">
         <label for="last_name">Last Name</label>
       </div>
     </div>
     <div class="input-field col s12">
       <input id="user_name" type="text" class="validate" name="username">
       <label for="user_name">User Name</label>
     </div>
     <div class="row">
       <div class="input-field col s12">
         <input id="password" type="password" class="validate" name="password">
         <label for="password">Password</label>
       </div>
     </div>
     <div class="row">
       <div class="input-field col s12">
         <input id="email" type="email" class="validate">
         <label for="email">Email</label>
       </div>
     </div>
     <div class="row">
       <div class="form-group">

         <button class="btn center"type="submit" name="button">Register</button>
       </div>

     </div>
   </form>
   <div class="col s2">

   </div>
 </div>
</div>


<% include ../partials/footer %>
