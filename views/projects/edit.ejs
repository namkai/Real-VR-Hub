<% include ../partials/header %>
<div class="container">
  <div class="row">
<h1 style="text-align: center">Edit <%= project.name%></h1>
<div style="width: 30%; margin: 25px auto;">
  <form action="/projects/<%=project._id%>?_method=PUT" method="POST">
    <div class="form-group">

    <input class="form-control"type="text" value="<%=project.name%>" name="project[name]" >
  </div>
  <div class="form-group">
    <input class="form-control"type="text" value="<%=project.image%>" name="project[image]" >
  </div>
  <div class="form-group">
    <input class="form-control"type="text" value="<%=project.project%>" name="project[project]" >
  </div>
  <div class="form-group">
    <input class="form-control"type="text" value="<%=project.description%>" name="project[description]" >
  </div>
  <div class="form-group">

    <button class="btn btn-lg btn-primary btn-block"type="submit" name="button">Submit!</button>
  </div>
  </form>
  <br>
  <a href="/projects">Go Back</a>
</div>
</div>
</div>


<% include ../partials/footer %>
